_G.key = "GZH2K4M5N7Q8R9TBUCVE"
_G.KeyInput = "string"

function MakeScriptHub()
	if game.PlaceId == 3101667897 then -- Legends Of Speed
	loadstring(game:HttpGet('https://raw.githubusercontent.com/kimbenner/script/main/alphaHub/R9SATBVDWE'))()
		
	elseif game.PlaceId == 3232996272 then -- Legends Of Speed ( Space )
	loadstring(game:HttpGet('https://raw.githubusercontent.com/kimbenner/script/main/alphaHub/8R9SAUCVDW'))()
		
	elseif game.PlaceId == 3276265788 then -- Legends Of Speed ( Space desert )
	loadstring(game:HttpGet('https://raw.githubusercontent.com/kimbenner/script/main/alphaHub/H2J3M5N6P8'))()
			
	elseif game.PlaceId == 8888615802 then -- Rainbow friends
	loadstring(game:HttpGet('https://raw.githubusercontent.com/kimbenner/script/main/alphaHub/4N6P7Q9SAT'))()
			
	elseif game.PlaceId == 9492364312 then -- jump race
	loadstring(game:HttpGet('https://raw.githubusercontent.com/kimbenner/script/main/alphaHub/EXFYG2J3K4'))()

    elseif game.PlaceId == 7047488135 then -- Speed Run Simulator
	loadstring(game:HttpGet("https://raw.githubusercontent.com/kimbenner/script/main/alphaHub/4M5P7Q8RAT"))()
			
	elseif game.PlaceId == 8540346411 then -- Rebirth Champions X
	loadstring(game:HttpGet("https://raw.githubusercontent.com/kimbenner/script/main/alphaHub/9TBUCVDXFY"))()
		
	elseif game.PlaceId == 155615604 then -- Prison-Life
	loadstring(game:HttpGet("https://raw.githubusercontent.com/kimbenner/script/main/alphaHub/P7R9SATCVD"))()
	
	elseif game.PlaceId == 7009799230 then -- Pressure Wash Simulator
	loadstring(game:HttpGet("https://raw.githubusercontent.com/kimbenner/script/main/alphaHub/YGZH2K4M5N"))()

	
	end
end

function CorrectKeyNotification()
	OrionLib:MakeNotification({
		Name = "Correct Key!",
		Content = "You Have Entered The Correct Key!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end
--print("[Alpha Loader] - [2/3] Key Checking...")

function IncorrectKeyNotification( ... )
	OrionLib:MakeNotification({
		Name = "Incorrect Key!",
		Content = "You Have Entered The Incorrect Key!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
      	if _G.KeyInput == _G.key then
		MakeScriptHub()
		--print("[Alpha Loader] - [3/3] Successfully executed!")
		--print(string.format("Elapsed time: %.3f", os.clock() - x))
		CorrectKeyNotification()
		OrionLib:Destroy()
		else
			IncorrectKeyNotification()
		end
  	end    
})

